@model IEnumerable<DataLayer.Models.Page>

<header><i class="icons icon-star"></i> <h3> پربازدیدترین اخبار </h3></header>
<ul>
    @foreach (var item in Model)
    {
        <li>
            <img src="~/Content/img/thumb.png" />
            <h2> <a href="/News/@item.PageId">@item.TitlePage</a></h2>
            <span>انتشار : @News.Tools.PersianDateConverter.ToShamsi(item.CreatDateTime)</span>
        </li>
    }
</ul>